#include<stdio.h>
void main()
{
    int arr[2][2],i,j;
    printf("enter element in array");
    for (i=0; i<2;i++)
    {
        for(j=0; j<2;j++)
        {
            scanf("%d",&arr[i][j]);
        }
    }
    printf("elements in array\n");
        for (i=0; i<2;i++)
        {
        for(j=0; j<2;j++)
        {
            printf("%d ",arr[i][j]);
        }
        printf("\n");
       }
}
